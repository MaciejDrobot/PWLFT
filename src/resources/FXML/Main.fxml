<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="650.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.controller.printing.PrintingController">
    <children>
       <TabPane prefHeight="650.0" prefWidth="1000.0" tabClosingPolicy="UNAVAILABLE">
                <Tab text="Entry">
                    <content>
                        <fx:include fx:id="Entry" source="entry/EntryTab.fxml" />
                    </content>
                </Tab>
                <Tab text="Stats">
                    <content>
                        <fx:include  source="Stats.fxml" />
                    </content>
                </Tab>
         <Tab text="Print" fx:id="printing">
             <content>
               <fx:include source="PrintingPane.fxml"/>
           </content>
         </Tab>
       </TabPane>
    </children>
</AnchorPane>
